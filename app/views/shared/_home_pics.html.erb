<% @homepicpath = image_path ENV['HOMEPIC'] %>
<% @homepicpathmobile = image_path ENV['HOMEPICMOBILE'] %>
<% if @user %>
  <% if  @user.productphoto.present? %>
    <% @homepicpath = cl_image_path @user.productphoto.path %>
  <% end %>
  <% if  @user.productphotomobile.present? %>
    <% @homepicpathmobile = cl_image_path @user.productphotomobile.path %>
  <% end %>
<% end %>
